{
    "buildCommand": "pnpm build",
    "installCommand": "pnpm install",
    "framework": "astro",
    "headers": [
        {
            "source": "/(.*).(avif|webp|svg|png|jpg)",
            "headers": [
            {
                "key": "Cache-Control",
                "value": "public, stale-while-revalidate=120"
            }
            ]
        }
    ]
}